<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.example.springboot05.mapper.DepartmentMapper">

    <select id="queryDepartmentList" resultType="Department">
        select * from company.department;
    </select>

    <select id="departmentById" resultType="Department">
        select * from company.department where id=#{id};
    </select>


</mapper>